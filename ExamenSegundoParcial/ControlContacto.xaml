             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExamenSegundoParcial"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    
    <StackPanel>

        <TextBlock Margin="8,8,8,0" FontSize="22" HorizontalAlignment="Center" FontWeight="Bold" FontFamily="Simplex">2.Contacto</TextBlock>

        <TextBlock Margin="8,8,8,0" FontFamily="Tw Cen MT Condensed" FontSize="20">Correo:</TextBlock>
        <TextBox Name="txtCorreo" Margin="8,8,534,0"></TextBox>

        <TextBlock Margin="8,8,8,0" FontFamily="Tw Cen MT Condensed" FontSize="20">Telefono:</TextBlock>
        <TextBox Name="txtTelefono" Margin="8,8,534,0"></TextBox>

        <TextBlock Margin="8,8,8,0" FontFamily="Tw Cen MT Condensed" FontSize="20">Celular:</TextBlock>
        <TextBox Name="txtCelular" Margin="8,8,534,0"></TextBox>
    
    </StackPanel>

</UserControl>
